{
  "name": "safen",
  "version": "2.3.0",
  "description": "Super Fast Object Validator for Javascript(& Typescript).",
  "keywords": [
    "validation",
    "validator",
    "validate",
    "assert",
    "check",
    "interface",
    "type",
    "safen",
    "schema",
    "jsonschema",
    "joi",
    "ajv",
    "typescript"
  ],
  "author": "Changwan Jun <wan2land@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/wan2land/safen.git"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "prepack": "npm run test && npm run build",
    "build": "rimraf ./lib && tsc --emitDeclarationOnly && babel src --out-dir lib --extensions \".ts\" --source-maps inline",
    "test": "npm run build && npm run test:lint && npm run test:unit",
    "test:lint": "eslint src/**/**/*.ts",
    "test:unit": "jest test"
  },
  "dependencies": {
    "core-js": "^3.10.1"
  },
  "devDependencies": {
    "@babel/cli": "7.14.8",
    "@babel/core": "7.14.8",
    "@babel/plugin-proposal-class-properties": "7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "7.14.7",
    "@babel/preset-env": "7.14.9",
    "@babel/preset-typescript": "7.14.5",
    "@types/jest": "26.0.24",
    "@types/node": "14.17.8",
    "eslint": "7.32.0",
    "eslint-config-stable": "0.9.0",
    "jest": "27.0.6",
    "rimraf": "3.0.2",
    "ts-jest": "27.0.4",
    "typescript": "4.3.5"
  }
}
